<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray_f6">


    <RelativeLayout
        android:id="@+id/rl_avatar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tb_title"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="10dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/avatar"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/v_avatar_ind"
            android:layout_width="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <ImageView
            android:id="@+id/iv_avatar"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_toLeftOf="@id/v_avatar_ind"
            android:layout_marginRight="14dp"
            android:layout_centerVertical="true" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_avatar"
        android:background="@color/gray_d" />


    <View
        android:id="@+id/v_sep_h3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/v_sep_h2"
        android:background="@color/gray_d" />

    <RelativeLayout
        android:id="@+id/rl_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h3"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/name"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_name_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_mobile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_name_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_name"
        android:background="@color/gray_d" />


    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h4"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/title"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_title_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_tile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_title_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>
    <View
        android:id="@+id/v_sep_h6_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_title"
        android:layout_marginLeft="12dp"
        android:background="@color/gray_d" />

    <RelativeLayout
        android:id="@+id/rl_department"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h6_1"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/department"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_department_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_department"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_department_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h6_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_department"
        android:layout_marginLeft="12dp"
        android:background="@color/gray_d" />

    <RelativeLayout
        android:id="@+id/rl_hospitall"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h6_2"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/hospitall"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_hospitall_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_hospitall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_hospitall_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h6_3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_hospitall"
        android:layout_marginLeft="12dp"
        android:background="@color/gray_d" />


    <RelativeLayout
        android:id="@+id/rl_business_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h6_3"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/business_card"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_business_card_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_business_card"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_business_card_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h6_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_business_card"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="15dp"
        android:background="@color/gray_d" />

    <RelativeLayout
        android:id="@+id/rl_good"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h6_5"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/good"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_good_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_good"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_good_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h6_4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_good"
        android:layout_marginLeft="12dp"
        android:background="@color/gray_d" />


    <RelativeLayout
        android:id="@+id/rl_practice_experience"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/v_sep_h6_4"
        android:background="@color/mdtp_white"
        android:clickable="true"
        android:paddingBottom="16dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/practice_experience"
            android:textColor="@color/gray_3"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/iv_practice_experience_ind"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_open"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/tv_practice_experience"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/iv_practice_experience_ind"
            android:layout_marginRight="14dp"
            android:contentDescription="TODO-DEL"
            android:textColor="@color/gray_9"
            android:textSize="13sp" />
    </RelativeLayout>

    <View
        android:id="@+id/v_sep_h6_6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/separator_width"
        android:layout_below="@id/rl_practice_experience"
        android:layout_marginLeft="12dp"
        android:background="@color/gray_d" />
</RelativeLayout>